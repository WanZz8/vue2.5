.header_outher
  z-index 200
  position absolute
  right 0
  left 0
  height 50px
  line-height 50px
  padding 0 12px
  text-align center
  -webkit-backface-visibility hidden
  backface-visibility hidden
  display flex
  display -webkit-box
  display -moz-box
  display -ms-flexbox
  display -webkit-flex
  section.home_left
    width 20%
    flex 0 0 20%
    box-flex 0 0 20%
    -webkit-box-flex 0 0 20%
    -moz-box-flex 0 0 20%
    -webkit-flex 0 0 20%
    position relative
    .home_back_icon
      width 50px
      height 50px
      position relative
      .icon_Back
        width 20px
        height 28px
        display block
        position absolute
        top 10px
        svg
          width 100%
          height 100%
          fill #fff
  section.home_center
    width 60%
    flex 0 0 60%
    box-flex 0 0 60%
    -webkit-box-flex 0 0 60%
    -moz-box-flex 0 0 60%
    -webkit-flex 0 0 60%
  section.home_right
    width 20%
    flex 0 0 20%
    box-flex 0 0 20%
    -webkit-box-flex 0 0 20%
    -moz-box-flex 0 0 20%
    -webkit-flex 0 0 20%
    p
      color #ffffff
      font-size 14px
.header_Left
  position relative
  z-index 9999
