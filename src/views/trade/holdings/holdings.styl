.holdings_detail
  margin 0 auto
  width 343px
  margin-top 10px
  .oneKey
    background #F8F8F8
    height 71px
    margin-bottom 6px
    ul
      display flex
    li
      width 50%
      flex 0 0 50%
      p
        color #6B89FE
        font-size 16px
        margin 13px 12px 0
        i
          font-size 12px
        &.money
          color #FF5E4D
          margin 2px 17px
      &.oneKey_right
        span
          width 108px
          border-radius 50px
          background #6B89FE
          color #fff
          display block
          text-align center
          padding 10px 0
          margin 17px 0 0 30px
  .main-body
    overflow scroll
    .holdings_list
      width 320px
      margin 5px auto 13px
      position relative
      box-shadow 0 2px 4px 0 rgba(0, 0, 0, 0.06), 0 -2px 4px 0 rgba(0, 0, 0, 0.07)
      padding 0px 9px
      .holdings_title
        height 35px
        overflow hidden
        .holdings_left
          float left
          h1
            font-size 14px
            color #4a4a4a
            line-height 35px
            float left
            font-weight normal
          i
            font-style normal
            font-weight normal
            color #fff
            display inline-block
            border-radius 50%
            background #EA3826
            font-size 12px
            text-align center
            float left
            margin 6px 7px
          p
            font-size 12px
            color #4a4a4a
            line-height 34px
            float left
        .holdings_right
          float left
          margin-left 10px
          line-height 34px
          h3
            float left
            font-size 14px
            font-weight normal
            color #f5a623
          span
            color #9B9B9B
            font-size 12px
            margin-left 4px
        .right_triangle
          width 0px
          height 0px
          border-right 0px solid transparent
          border-left 34px solid transparent
          position absolute
          right 0
          top 0
          p
            position absolute
            top -32px
            right 3px
            color #fff
            font-size 12px
      .holdings_content
        display flex
        border-bottom 1px solid #f8f8f8
        .order_details

        .holdings_content_left
          flex 0 0 60%
          p
            float left
            margin 0 12px 6px 0
            line-height 28px
            font-size 12px
            color #9B9B9B
        .holdings_content_right
          flex 0 0 40%
          h2
            font-size 16px
            color #EA3826
            padding-top 16px
            i
              font-weight normal
              font-size 13px
              color #888
              display block
            span
              display block
          p
            color #9B9B9B
            font-size 12px
      .holdings_bottom
        line-height 24px
        display flex
        span
          width 84px
          text-align center
          flex 0 0 84px
          height 24px
          color #F5A623
          font-size 12px
          display block
          border-radius 3px
          margin 8px 10px
      .holdings_bottom_p
        height 35px
        line-height 35px
        text-align center
        width 100%
        color #6e6e6e
  .scroll-content
    padding-top 5px
