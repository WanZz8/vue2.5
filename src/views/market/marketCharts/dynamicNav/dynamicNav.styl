.marketNav
  .commodity_nav
    width 352px
    min-height 200px
    max-height 394px
    background #F9F9F9
    position fixed
    top 50px
    z-index 301
    padding 12px
    overflow hidden
    overflow-y auto
    .list-wrapper
      height 394px
      background none
    .commodity_list
      span
        width 106px
        height 24px
        line-height 28px
        color #eee
        display block
        text-align center
        border-radius 4px
        font-size 14px
        margin-left 16px
      p
        width 30%
        height 38px
        background #FFFFFF
        box-shadow 0 2px 4px 0 rgba(0, 0, 0, 0.06), 0 -2px 4px 0 rgba(0, 0, 0, 0.07)
        border-radius 5px
        margin 11px 1.66% 11px 1.66%
        text-align center
        line-height 42px
        font-size 14px
        color #4A4A4A
        display inline-block
        position relative
        i
          position absolute
          top -5px
          width 22px
          height 22px
          right -5px
          color #eee
          background #999999
          line-height 23px
          border-radius 50%
          font-size 10px
          opacity 0.8
  .market_Mask
    position fixed
    top 50px
    left 0
    width 100%
    height calc(100% - 50px)
    background rgba(0, 0, 0, 0.6)
    z-index 300
.move-enter-active, .move-leave-active
  transition all 0.3s ease-out
.move-enter
  transform translateY(40px)
  opacity 0
.move-leave-active
  transform translateY(40px)
  opacity 0
.fade-enter-active, .fade-leave-active
  transition all 0.2s ease-out
.fade-enter, .fade-leave-to
  opacity 0
