.linkMana_detail
  margin 0 auto
  background-color #F2F2F2
  position relative
  .commission_content
    width 100%
    box-sizing border-box
    overflow scroll
    .com_content
      background-color #FFF
      width 92%
      margin 0 auto
      margin-bottom 20px
      .commiss_header
        width 100%
        height 40px
        line-height 40px
        text-align left
        box-sizing border-box
        position relative
        // .icon_Invite
        // position absolute
        // height 30px
        // width 35px
        // padding-top 10px
        // padding-left 15px
        // box-sizing border-box
        p
          float left
          height 30px
          line-height 30px
          margin-left 15px
          margin-top 5px
          color #999
        .header-rt
          float right
          height 100%
          .icon_Invite
            float left
            height 23px
            margin-top 10px
            margin-right 5px
            width 30px
          b
            float left
            font-size 20px
            font-weight 100
            margin-left 5px
            margin-right 5px
            margin-top 2px
      .commis_type
        padding-left 15px
        padding-right 15px
        width 100%
        box-sizing border-box
        .commis_type_p1
          width 100%
          box-sizing border-box
          border-top 0.1px solid #f4f4f4
          border-bottom 0.1px solid #f4f4f4
          position relative
          display flex
          span
            font-family PingFang-SC-Medium
            font-size 12px
            color #9B9B9B
            padding-left 5px
            &.span1
              flex 0 0 20%
              line-height 40px
            &.span2
              flex 0 0 70%
              margin-left 5%
          .copys
            width 18px
            height 18px
            display block
            position absolute
            top 10px
            left 60px
            svg
              fill #666
        .commis_type_p2
          width 100%
          height 30px
          line-height 30px
          .span1
            font-family PingFang-SC-Medium
            font-size 12px
            color #9B9B9B
            padding-left 5px
            float left
          .span2
            font-family PingFang-SC-Medium
            font-size 12px
            color #9B9B9B
            padding-right 10px
            float right
      .commis-footer
        width 100%
        height 56px
        box-sizing border-box
        .commis-footer-p
          width 100%
          padding-left 25px
          padding-right 25px
          box-sizing border-box
          height 25px
          line-height 25px
          span
            display inline-block
            width 85px
            font-family PingFang-SC-Medium
            font-size 12px
            color #9B9B9B
            text-align center
    .modal-fade-enter, .modal-fade-leave-active
      opacity 0
    .modal-fade-enter-active, .modal-fade-leave-active
      transition opacity 0.5s ease
    .modal-backdrop
      position fixed
      top 0
      right 0
      bottom 0
      left 0
      background-color rgba(0, 0, 0, 0.3)
      display flex
      justify-content center
      flex-direction column
      align-items center
      z-index 1000
      .modal
        background-color #fff
        box-shadow 2px 2px 20px 1px
        overflow hidden
        display flex
        flex-direction column
        width 335px
        height 345px
        margin-top 65px
        border-radius 5px
        position relative
        .modal-header
          padding 5px
          display flex
          justify-content space-between
          .modal-close
            height 30px
            line-height 30px
            text-align center
            width 100%
            font-size 15px
        .modal-body
          position relative
          margin 15px 0
          .code
            width 100%
            height 205px
            text-align center
        .modal-footer
          justify-content flex-end
          padding-left 5px
          padding-right 5px
          .conf_link_p
            width 100%
            margin-top 5px
            .conf_link
              color #6B89FE
      .phoneCode_btn
        width 335px
        height 45px
        line-height 45px
        background #6B89FE
        margin-top 20px
        border-radius 15px
        text-align center
        color #fff
  .pop_con
    width 100%
    height 165px
    // margin-bottom 20px
    .pop_b_header
      width 100%
      height 45px
      position relative
      .pop_p1
        width 100%
        text-align center
        font-family PingFang-SC-Medium
        font-size 14px
        color #4A4A4A
        height 45px
        line-height 45px
      .pop_p2
        position absolute
        width 18px
        height 18px
        top 12px
        right 12px
  .top_title
    height 35px
    position relative
    text-align center
    line-height 35px
    .icon_spinner
      position absolute
      height 35px
      width 35px
      left 90px
.top_title
  width 100%
  position relative
  height 35px
  line-height 35px
  text-align center
  .icon_spinner
    position absolute
    height 35px
    width 35px
    left 90px
#qrcode
  width 160px
  height 160px
  margin 15px auto
.modal-footer
  text-align center
.noData_icon
  padding-top 60px
  i
    width 150px
    height 150px
    margin 0px auto 0
    display block
    svg
      fill #ddd
  p
    text-align center
    font-size 20px
    color #ddd
    font-weight 600
