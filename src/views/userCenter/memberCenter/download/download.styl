.download_view
  -webkit-box-sizing border-box
  box-sizing border-box
  .bgContainer
    width 100%
    height 100%
    background url('../../../../assets/images/memberCenter/downloadPage.jpg') no-repeat center
    background-size 100% 100%
    position relative
    .download_con
      position fixed
      bottom 100px
      height 35px
      line-height 35px
      text-align center
      width 100%
      .download_app
        width 100%
        height 100%
        position relative
        .start
          top 50%
          left 50%
          transform translate(-50%,-50%)
          position absolute
          width 85%
          border 1px solid #fff
          border-radius 5px
          color #fff
        .start:active
          border 1px solid #e8e8e8
          opacity 0.8
