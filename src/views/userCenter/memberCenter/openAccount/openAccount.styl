.open_view
  .dis_content
    margin-top 30px
    .open_navBar
      margin 0 auto
      background-color #FFF
      padding-top 20px
      height 70px
      .mint-navbar
        width 343px
        height 55px
        box-shadow 0 2px 4px 0 rgba(0, 0, 0, 0.06), 0 -2px 4px 0 rgba(0, 0, 0, 0.07)
        border-radius 25px
        margin-top 20px
        margin 0px auto 0px auto
        box-sizing border-box
        .mint-tab-item
          height 21px
          display inline-block
          margin-left 15px
          margin-right 15px
          .mint-tab-item-label
            height 21px
            line-height 21px
        div
          // line-height 8px
          // font-size 14px
        .is-selected
          // border none
          // position relative
          &::before
            content ''
            width 50%
            height 3px
            position absolute
            left 50%
            bottom 0
            margin-left -25%
    .reRocd_content
      width 100%
      .top_title
        width 100%
        position relative
        height 35px
        line-height 35px
        text-align center
        .icon_spinner
          position absolute
          height 35px
          width 35px
          left 90px
