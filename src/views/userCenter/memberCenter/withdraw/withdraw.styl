  .part2
    width 100%
    // height 139px
    position relative
    .item_wrap
      box-sizing border-box
      padding-left 12px
      padding-right 12px
      .item_styl
        width 100%
        height 100%
        box-sizing border-box
        background-color #fff
        padding 0 15px
        .item_p1
          height 36px
          line-height 36px
        .item_p2
          height 44px
          font-family PingFang-SC-Medium
          font-size 24px
          color #4A4A4A
          border-bottom 1px solid #f8f8f8
          position relative
          .mint-cell-wrapper
            background-image none
            height 35px
            padding 0
            & input::placeholder
              font-family PingFang-SC-Medium
              font-size 14px
          input
            height 44px
            line-height 44px
            border-bottom 1px solid #f8f8f8
            padding-left 26px
            padding-right 10px
            outline none
            font-size 20px
            color #666666
            background #fff
          i
            position absolute
            top 5px
            left 0
            color #666666
            font-size 24px
        .item_p3
          font-family PingFang-SC-Medium
          font-size 14px
          color #9B9B9B
          line-height 40px
      .item_styl::before
        display table
        content ''
