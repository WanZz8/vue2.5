            line-height 20px
            color #D0031B
  .part4
    padding-left 15px
    padding-right 15px
    height 45px
    margin-top 25px
    margin-bottom 150px
    .withdraw_ss
      width 100%
      height 100%
      line-height 45px
      display block
      border-radius 25px
      font-size 15px
      background-color #6B89FE
      text-align center
      color #fff
      outline 0
      border none
      touch-action none
      position relative
      overflow hidden
    .withdraw_ss:after
      content ''
      display block
      position absolute
      width 100%
      height 100%
      top 0
      left 0
      pointer-events none
      background-image radial-gradient(circle, #767676 10%, transparent 10.01%)
      background-repeat no-repeat
      background-position 50%
      transform scale(10, 10)
      opacity 0
      transition transform 0.3s, opacity 0.5s
      height 45px
      line-height 45px
      padding-right 15px
    .withdraw_ss:active:after
      transform scale(0, 0)
      opacity 0.3
      transition 0s
